{
    "name": "adamhut/chuck-norris-jokes",
    "description": "A package return random jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Huang",
            "email": "adamhut@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
 
    "autoload": {
        "psr-4": {
            "Adamhut\\ChuckNorrisJokes\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adamhut\\ChuckNorrisJokes\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adamhut\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "Adamhut\\DebugbChuckNorrisJokes\\Facades\\ChuckNorris"
            }
        }
    }

}
